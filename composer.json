{
  "name": "flairuk/laravel-countries",
  "description": "Laravel Countries is a bundle for Laravel, providing Almost ISO 3166_2, 3166_3, currency, Capital and more for all countries.",
  "authors": [
    {
      "name": "Phil Graham ",
      "email": "phil.graham@ht2labs.com",
      "homepage": "https://www.ht2labs.com/",
      "role": "developer"
    },
    {
      "name": "Phil Graham",
      "role": "developer"
    }
  ],
  "keywords": [
    "laravel",
    "countries",
    "iso_3166_2",
    "iso_3166_3"
  ],
  "homepage": "https://github.com/flairuk/laravel-countries",
  "license": [
    "MIT"
  ],
  "require": {
    "php": ">=5.3.0"
  },
  "autoload": {
    "psr-0": {
      "FLAIRUK\\Countries": "src/"
    },
    "classmap": [
      "src/commands"
    ]
  },
  "minimum-stability": "dev",
  "extra": {
    "laravel": {
      "providers": [
        "FLAIRUK\\Countries\\CountriesServiceProvider"
      ],
      "aliases": {
        "Countries": "FLAIRUK\\Countries\\CountriesFacade"
      }
    }
  },
  "support": {
    "source": "https://github.com/flairuk/laravel-countries",
    "issues": "https://github.com/flairuk/laravel-countries/issues"
  }
}
